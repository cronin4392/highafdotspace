{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:css && npm run watch:css & node server.js",
    "postinstall": "npm run build:css && webpack -p --config webpack.prod.config.js",
    "build:css": "node ./scripts/build_css.js",
    "watch:css": "chokidar-cmd -c 'npm run build:css' -t ./src/style",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.js'",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "chokidar-cmd": "^1.2.1",
    "combine-mq": "^0.8.1",
    "express": "^4.13.4",
    "fs": "0.0.2",
    "jquery": "^2.2.3",
    "lodash": "^3.10.1",
    "moment": "^2.13.0",
    "node-sass": "^3.5.3",
    "react": "^0.14.3",
    "react-addons-update": "^15.0.1",
    "react-dom": "^0.14.3",
    "react-iframe": "0.0.4",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
